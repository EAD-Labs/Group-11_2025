{
  "name": "Goqualify",
  "description": "Goqualify",
  "version": "1.0.0",
  "manifest_version": 3,
  "key": "",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Goqualify",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "tabs", "activeTab","identity","scripting","cookies","notifications"],
 "host_permissions": [
    "https://www.youtube.com/*",
    "http://localhost:4567/*",
    "https://localhost:4567/*",
    "http://localhost:3232/*",
    "https://localhost:3232/*"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["http://localhost:3232/*"]
  }
}
